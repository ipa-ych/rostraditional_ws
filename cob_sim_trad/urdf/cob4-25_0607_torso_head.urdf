      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.038 0.038 0.03"/>
      </geometry>
    </collision>
  </link>
  <joint name="head_cam_frame_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="head_cam_link"/>
    <child link="head_cam_frame"/>
  </joint>
  <link name="head_cam_frame"/>
  <joint name="head_cam_optical_frame_joint" type="fixed">
    <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0"/>
    <parent link="head_cam_frame"/>
    <child link="head_cam_optical_frame"/>
  </joint>
  <link name="head_cam_optical_frame"/>
  <gazebo reference="head_cam_link">
    <sensor name="head_cam" type="wideanglecamera">
      <update_rate>10</update_rate>
      <camera name="head_cam">
        <horizontal_fov>2.9670597283903604</horizontal_fov>
        <image>
          <format>B8G8R8</format>
          <width>1280</width>
          <height>960</height>
        </image>
        <clip>
          <near>0.02</near>
          <far>300</far>
        </clip>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.007</stddev>
        </noise>
        <lens>
          <type>custom</type>

          <custom_function>
            <c1>1.05</c1>

            <c2>4</c2>

            <f>1.0</f>

            <fun>tan</fun>

          </custom_function>

          <scale_to_hfov>true</scale_to_hfov>

          <cutoff_angle>3.141592653589793</cutoff_angle>

          <env_texture_size>512</env_texture_size>
        </lens>
      </camera>
      <plugin filename="libgazebo_ros_camera.so" name="camera_controller">
        <alwaysOn>true</alwaysOn>
        <updateRate>10</updateRate>
        <cameraName>head_cam</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>head_cam_optical_frame</frameName>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
    <material>IPA/LightGrey</material>
  </gazebo>

  <!-- load ros2_control config file -->
  <gazebo>
    <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
      <parameters>$(find cob_sim_trad)/config/cob_arm_0415.yaml</parameters>
    </plugin>
  </gazebo>
  
</robot>
